<Window x:Class="Kamishibai.Wpf.Demo.View.ChildWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="ChildWindow" Height="450" Width="800">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Content="Open Window" Margin="5" Command="{Binding OpenWindowCommand}"/>
        <Button Content="Open Dialog" Margin="5" Command="{Binding OpenDialogCommand}"/>
    </StackPanel>
</Window>
